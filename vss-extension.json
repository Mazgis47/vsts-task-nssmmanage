                                "alwaysRun": false,
                                "continueOnError": false,
                                "timeoutInMinutes": 0,
                                "definitionType": "task",
                                "overrideInputs": {},
                                "condition": "succeeded()",
                                "inputs": {
                                    "SourceFolder": null,
                                    "Contents": "**",
                                    "TargetFolder": "$(DefaultInstalationDir)\\$(StoneCo.ServiceName)\\",
                                    "CleanTargetFolder": "false",
                                    "OverWrite": "false",
                                    "flattenFolders": "false"
                                }
